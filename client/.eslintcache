v\" expand=\"lg\">\n      <Navbar.Brand className=\"nav-header\">BW</Navbar.Brand>\n      <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\n      <Navbar.Collapse id=\"basic-navbar-nav\">\n        <Nav className=\"nav-links\" className=\"mr-auto\">\n          <Nav.Link name=\"home\" onClick={(e) => scroll(e)}>Home</Nav.Link>\n          <Nav.Link name=\"about\" onClick={(e) => scroll(e)}>About</Nav.Link>\n          <Nav.Link name=\"projects\" onClick={(e) => scroll(e)}>Projects</Nav.Link>\n          <Nav.Link name=\"contact\" onClick={(e) => scroll(e)}>Contact</Nav.Link>\n        </Nav>\n      </Navbar.Collapse>\n    </Navbar>\n  );\n};\n\nexport default MainNav;\n","/Users/bradleywilson06/Documents/React-Portfolio/client/src/components/MessageStatus.js",[],{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":30,"column":13,"nodeType":"59","endLine":30,"endColumn":75},{"ruleId":"57","severity":1,"message":"58","line":43,"column":13,"nodeType":"59","endLine":43,"endColumn":78},{"ruleId":"57","severity":1,"message":"58","line":56,"column":13,"nodeType":"59","endLine":59,"endColumn":14},{"ruleId":"60","severity":1,"message":"61","line":1,"column":17,"nodeType":"62","messageId":"63","endLine":1,"endColumn":25},{"ruleId":"60","severity":1,"message":"64","line":2,"column":10,"nodeType":"62","messageId":"63","endLine":2,"endColumn":23},{"ruleId":"57","severity":1,"message":"58","line":45,"column":96,"nodeType":"59","endLine":45,"endColumn":154},{"ruleId":"60","severity":1,"message":"65","line":2,"column":23,"nodeType":"62","messageId":"63","endLine":2,"endColumn":29},{"ruleId":"60","severity":1,"message":"66","line":6,"column":10,"nodeType":"62","messageId":"63","endLine":6,"endColumn":22},{"ruleId":"67","severity":1,"message":"68","line":41,"column":36,"nodeType":"69","endLine":41,"endColumn":55},"no-native-reassign",["70"],"no-negated-in-lhs",["71"],"react/jsx-no-target-blank","Using target=\"_blank\" wi